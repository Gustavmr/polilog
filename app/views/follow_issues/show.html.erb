<div class="page-header">
  <h3>Follow Issue #<%= @follow_issue.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @follow_issue.user_id %></dd>

      <dt>Issue</dt>
      <dd><%= @follow_issue.issue_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/follow_issues" class="btn btn-primary">
        Back
      </a>
      <a href="/follow_issues/<%= @follow_issue.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_follow_issue/<%= @follow_issue.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
